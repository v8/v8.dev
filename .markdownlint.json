{
  "default": true,
  "heading-style": { "style": "atx" },
  "ul-style": { "style": "dash" },
  "ul-indent": { "indent": 4 },
  "line-length": false,
  "single-title": { "front_matter_title": "" },
  "no-trailing-punctuation": { "punctuation": ".,;:!" },
  "no-inline-html": {
    "allowed_elements": [
      "a",
      "abbr",
      "b",
      "code",
      "div",
      "em",
      "figcaption",
      "figure",
      "i",
      "iframe",
      "img",
      "kbd",
      "pre",
      "sub",
      "sup",
      "video"
    ]
  },
  "fenced-code-language": false,
  "no-alt-text": false
}
